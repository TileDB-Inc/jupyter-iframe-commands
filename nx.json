{
  "namedInputs": {
    "default": ["{projectRoot}/**/*"]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [
        "{projectRoot}/packages/extension/lib",
        "{projectRoot}/packages/host/lib"
      ]
    },
    "build:prod": {
      "cache": true,
      "dependsOn": ["^build:prod"],
      "outputs": [
        "{projectRoot}/packages/extension/lib",
        "{projectRoot}/packages/host/lib"
      ]
    }
  }
}
