{
  "name": "jupyterlab-iframe-commands-extension",
  "version": "0.1.0",
  "description": "\"A JupyterLab extension to facilitate integration with a host page via an IFrame\"",
  "homepage": "https://github.com/TileDB-Inc/jupyter-iframe-commands#readme",
  "bugs": {
    "url": "https://github.com/TileDB-Inc/jupyter-iframe-commands/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TileDB-Inc/jupyter-iframe-commands.git"
  },
  "license": "BSD-3-Clause",
  "author": "QuantStack",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js",
    "src/**/*.{ts,tsx}"
  ],
  "sideEffects": [
    "style/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc -b",
    "watch": "tsc -b --watch"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "../../jupyter_iframe_commands/labextension"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.3.1",
    "@jupyterlab/settingregistry": "^4.3.1",
    "jupyterlab-iframe-commands-host": "0.1.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.2.6"
  }
}
